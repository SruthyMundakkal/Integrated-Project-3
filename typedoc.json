{
    "entryPoints": ["./app", "./lib", "./components", "./utils"],
    "out": "./docs",
    "entryPointStrategy": "expand",
    "tsconfig": "./tsconfig.json",
    "name": "Employee Claim System Documentation",
    "readme": "./README.md",
    "excludePrivate": true,
    "excludeExternals": true,
    "hideGenerator": true,
    "visibilityFilters": {
        "protected": true,
        "private": false,
        "inherited": true,
        "external": false
    },
    "categorizeByGroup": true,
    "searchInComments": true,
    "validation": {
        "invalidLink": true,
        "notExported": true
    }
}